@{
    ViewData["Title"] = "Delete Application";
}
@model MovieResponse
<h2>Confirm Deletion</h2>
<h3>Are you sure that you want to delete the record for @Model.Title?</h3>
<form method="post" asp-action="Delete">
    <button type="submit">Delete</button>
    <a asp-controller="Home" asp-action="MovieList" class="btn">Cancel</a>

    @*hidden asp-for*@
    <input type="hidden" asp-for="MovieId" />
</form>